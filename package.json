{
  "name": "mmimportjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "ts-node --transpile-only src/index.ts",
    "run": "ts-node --transpile-only",
    "build": "npm run build-default",
    "build-dist": "npm run build-chunk-linux",
    "build-default": "npm run tsc && pkg --targets node16-linux-x64 --out-path executables",
    "build-chunk-linux": "npm run build-linux dist/chunk-posts.js",
    "build-chunk-macos": "npm run build-macos dist/chunk-posts.js",
    "build-macos": "npm run tsc && pkg --targets node16-macos-x64,node16-macos-arm64,node16-linux-x64,node16-linux-arm64 --out-path executables",
    "build-linux": "npm run tsc && pkg --targets node16-linux-x64 --out-path executables",
    "tsc": "tsc",
    "check-types": "npm run tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.1.5",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
